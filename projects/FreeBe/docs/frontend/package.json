{
  "name": "webclient",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:pre": "vue-tsc --noEmit && vite build --mode staging",
    "build:pro": "vue-tsc --noEmit && vite build --mode production",
    "serve": "yarn run build:vite && serve -s dist",
    "preview": "vite preview",
    "prepare": "husky install",
    "prettier-format": "prettier --config .prettierrc.cjs \"src/**/*.{vue,js,ts}\" --write"
  },
  "dependencies": {
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@headlessui/vue": "^1.7.4",
    "@heroicons/vue": "^2.0.13",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "^18.11.12",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vee-validate/zod": "^4.7.3",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vueuse/core": "^9.6.0",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.13",
    "axios": "^1.2.0",
    "ethers": "^5.7.2",
    "fast-glob": "^3.2.12",
    "husky": "^8.0.2",
    "less": "^4.1.3",
    "lint-staged": "^13.0.4",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.27",
    "pinia-plugin-persistedstate": "^3.0.1",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "tailwindcss": "^3.2.4",
    "tailwindcss-dark-mode": "^1.1.7",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.12.0",
    "unplugin-vue-components": "^0.22.11",
    "vee-validate": "^4.7.3",
    "vite": "^3.2.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^3.3.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.9",
    "vuetify": "3.0.3",
    "zod": "^3.19.1"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "npm run prettier-format"
    ]
  }
}
